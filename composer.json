{
    "name": "wangdong/cumuli",
    "description": "Cumuli系统",
    "keywords": [
        "wangdong",
        "cumuli"
    ],
    "authors": [
        {
            "name": "wangdong",
            "email": "mail@wangdong.io"
        }
    ],
    "license": [
        "MIT",
        "GPL"
    ],
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "barryvdh/laravel-debugbar": "2.4.*",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.0",
        "laravel/tinker": "~1.0",
        "mnabialek/laravel-sql-logger": "^1.1",
        "predis/predis": "^1.1",
        "reactivex/rxphp": "^2.0",
        "rx/child-process": "^1.0",
        "rx/websocket": "^1.0",
        "wangdong/cumuli-module-system-config": "^1.0",
        "wangdong/cumuli-module-system-flow": "^1.0",
        "wangdong/cumuli-module-system-page": "^1.0",
        "wangdong/cumuli-module-system-role": "^1.0",
        "wangdong/cumuli-module-system-structure": "^1.0",
        "wangdong/cumuli-module-system-user": "^1.0"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "files": [
            "app/Support/helpers.php"
        ],
        "classmap": [
            "database",
            "module"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize",
            "php artisan module:cache"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize",
            "php artisan module:cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
